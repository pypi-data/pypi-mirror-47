---
docker:
  # Specifies the Docker registry to which the application image is
  # uploaded by default.
  registry: {{ spec.docker.registry }}

  # The name of the repository holding the image.
  repository: {{ spec.docker.repository }}

  # Commands to run in the build image(s). These may be used
  # to install OS-managed packages (e.g. apt, yum, apk) or
  # other commands to prepare the builders. Note that this
  # setting is only applicable if the project compiles
  # a container image using the QSA.
  commands: []
