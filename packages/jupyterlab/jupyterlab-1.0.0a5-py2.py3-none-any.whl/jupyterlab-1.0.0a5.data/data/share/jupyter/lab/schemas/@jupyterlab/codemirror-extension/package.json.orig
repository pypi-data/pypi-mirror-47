    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.8",
    "@jupyterlab/codeeditor": "^1.0.0-alpha.8",
    "@jupyterlab/codemirror": "^1.0.0-alpha.8",
    "@jupyterlab/coreutils": "^3.0.0-alpha.8",
    "@jupyterlab/docregistry": "^1.0.0-alpha.8",
    "@jupyterlab/fileeditor": "^1.0.0-alpha.8",
    "@jupyterlab/mainmenu": "^1.0.0-alpha.8",
    "@jupyterlab/statusbar": "^1.0.0-alpha.8",
    "@phosphor/widgets": "^1.6.0",
    "codemirror": "~5.47.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
