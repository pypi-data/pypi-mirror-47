# 1QBit Matplotlib style file

lines.linewidth : 1.2
lines.linestyle : -


patch.facecolor : 00aeef
patch.edgecolor : 1c2b39

font.family : sans-serif
font.size : 12.0
font.sans-serif : brandon_text_regular.otf, Brandon Text, DejaVu Sans
#, DejaVu Sans, Lucida Grande, Verdana, Geneva, Lucid, Arial, Helvetica, Avant Garde, sans-serif
font.weight : 500

axes.titlesize: large
axes.titleweight: 500

axes.prop_cycle  : cycler('color', '00aeef,1c2b39,0075bc,72808a,67cdf2,003262')
axes.facecolor   : e6eef4
axes.edgecolor   : 1c2b39
axes.grid       :  True
axes.labelweight : 500
axes.xmargin : 0.05
axes.ymargin : 0.05

xtick.color : 1c2b39
ytick.color : 1c2b39
axes.labelcolor : 1c2b39
#figure.facecolor : c5ccd1

image.cmap : Blues


legend.fancybox : True
legend.facecolor: e6eef4
#legend.facealpha: 0.9
legend.framealpha: 0.9

grid.color : 72808a
grid.linestyle : : 
grid.linewidth : 0.25
grid.alpha : 1.0



savefig.facecolor   : f5fcff
savefig.dpi : 300



