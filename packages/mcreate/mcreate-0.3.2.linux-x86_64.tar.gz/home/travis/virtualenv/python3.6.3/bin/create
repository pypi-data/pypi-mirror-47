#!/home/travis/virtualenv/python3.6.3/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'mcreate==0.3.2','console_scripts','create'
__requires__ = 'mcreate==0.3.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mcreate==0.3.2', 'console_scripts', 'create')()
    )
