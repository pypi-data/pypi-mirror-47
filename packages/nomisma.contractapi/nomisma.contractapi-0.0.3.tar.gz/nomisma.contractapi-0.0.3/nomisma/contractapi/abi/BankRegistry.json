  "devdoc": {
    "methods": {
      "checkRole(address,string)": {
        "details": "reverts if addr does not have role",
        "params": {
          "addr": "address",
          "roleName": "the name of the role // reverts"
        }
      },
      "constructor": {
        "details": "bytes32 hashes of `Bank` and `BankState` are necessary to deploy this `BankRegistry` contract."
      },
      "deployBank(uint256[4],uint256[4],address,bytes)": {
        "details": "deployBank - deploys new bank contract",
        "params": {
          "_loanToken": "- address of bank funding currency",
          "data": "- bytes that represent collateral matrix for the bank.",
          "dateArgs": "- 0 - expirationDate - date when bank expire. 1 - lddInvestEquity - timestamp 2 - lddInvestDebt - timestamp 3 - lddBorrow - timestamp",
          "deployNumArgs": "- 0 - _minimumReserveRatio - percentage amount multiplied by RATE_PRECISION_MULTIPLIER which corresponds to minimum equity that should always remain in the bank as a percentage of total debt and equity investments made in this bank. 1 - commission - amount of commission relative to _srcAmount provided to comply minEquity rule. 2 - _debtInterest - percentage interest on debt investments 3 - _srcAmount - amount of equity user is willing to deploy within the context of transaction to deployBank. The value is provided in loanToken terms."
        }
      },
      "getEventEmitter()": {
        "details": "Returns address of an existing BankEventEmitter."
      },
      "hasRole(address,string)": {
        "details": "determine if addr has role",
        "params": {
          "addr": "address",
          "roleName": "the name of the role"
        },
        "return": "bool"
      },
      "setDateKeeper(address)": {
        "details": "Registers `DateKeeper` contract by assigning it's address to the `dateKeeper` variable. Only `owner` can call this function.",
        "params": {
          "_dateKeeper": "- address of a `DateKeeper` contract to be set."
        }
      },
      "setEventEmitter(address)": {
        "details": "Registers `EventEmitter` contract by assigning it's address to the `eventEmitter` variable. Only `governor` can call this function.",
        "params": {
          "_eventEmitter": "- address of a `EventEmitter` contract to be set."
        }
      },
      "setExchangeConnector(address)": {
        "details": "Registers `ExchangeConnector` contract by assigning it's address to the `exchangeConnector` variable. Only `governor` can call this function.",
        "params": {
          "_exchangeConnector": "- address of an `ExchangeConnector` contract to be set."
        }
      },
      "setMinEquityBalance(uint256)": {
        "details": "Sets new minEquityBalance Only `owner` can call this function.",
        "params": {
          "_minEquityBalance": "- new minimum equity balance in Ether."
        }
      },
      "setTokenManager(address)": {
        "details": "Registers `TokenManager` contract by assigning it's address to the `tokenManager` variable. Only `governor` can call this function.",
        "params": {
          "_tokenManager": "- address of a `TokenManager` contract to be set."
        }
      }
    }
