{
    "name": "@girder/large_image",
    "version": "1.0.0",
    "description": "Create, serve, and display large multiresolution images.",
    "homepage": "https://github.com/girder/large_image",
    "bugs": {
        "url": "https://github.com/girder/large_image/issues"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/girder/large_image.git"
    },
    "peerDependencies": {
        "@girder/core": "*"
    },
    "dependencies": {
        "copy-webpack-plugin": "^4.5.2",
        "d3": "^3.5.16",
        "hammerjs": "^2.0.8",
        "geojs": "^0.19.1",
        "sinon": "^2.1.0",
        "slideatlas-viewer": "4.0.3"
    },
    "main": "./index.js",
    "girderPlugin": {
        "name": "large_image",
        "main": "./main.js",
        "webpack": "webpack.helper"
    },
    "eslintConfig": {
        "extends": "girder",
        "rules": {
            "for-direction": "error",
            "getter-return": "error",
            "multiline-ternary": [
                "error",
                "always-multiline"
            ],
            "no-alert": "error",
            "switch-colon-spacing": "error",
            "import/exports-last": "error",
            "promise/no-native": "error",
            "promise/no-return-in-finally": "error",
            "promise/no-return-wrap": "error"
        },
        "root": true
    },
    "eslintIgnore": [
        "**/node_modules/"
    ]
}
