#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'imx==0.1.2','console_scripts','imxsd'
__requires__ = 'imx==0.1.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('imx==0.1.2', 'console_scripts', 'imxsd')()
    )
